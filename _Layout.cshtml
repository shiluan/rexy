<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AppDoc - Manage Applications</title>
    
    @Scripts.Render("https://code.jquery.com/jquery-1.9.1.js")
    

    
    <!--//bootstrap-->
    @Styles.Render("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css")
    @Styles.Render("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css")
    @Scripts.Render("https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js")

    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("AppDoc(1.0)", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Applications", "AppList", "Product")</li>
                    <li>@Html.ActionLink("Documents", "AdDocList", "Product")</li>
                    <li>@Html.ActionLink("Artifacts", "ArtifactList", "Product")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    

                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
      
    </div>
    

    <footer style="height: 40px;" class="panel-footer navbar-fixed-bottom">
        <div class="container">
            <p style="text-align: right;">&copy; @DateTime.Now.Year AppDoc (1.0) - Powered by ASP.NET</p>
        </div>
    </footer>
    


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
